{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "mainapp": {
      "projectType": "application",
      "schematics": {},
      "root": "projects/mainapp",
      "sourceRoot": "projects/mainapp/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/mainapp",
            "index": "projects/mainapp/src/index.html",
            "main": "projects/mainapp/src/main.ts",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "projects/mainapp/tsconfig.app.json",
            "assets": [
              "projects/mainapp/src/favicon.ico",
              "projects/mainapp/src/assets"
            ],
            "styles": [
              "projects/mainapp/src/styles.css"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "mainapp:build:production"
            },
            "development": {
              "browserTarget": "mainapp:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "mainapp:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "projects/mainapp/tsconfig.spec.json",
            "assets": [
              "projects/mainapp/src/favicon.ico",
              "projects/mainapp/src/assets"
            ],
            "styles": [
              "projects/mainapp/src/styles.css"
            ],
            "scripts": []
          }
        }
      }
    },
    "loginapp": {
      "projectType": "application",
      "schematics": {},
      "root": "projects/loginapp",
      "sourceRoot": "projects/loginapp/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/loginapp",
            "index": "projects/loginapp/src/index.html",
            "main": "projects/loginapp/src/main.ts",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "projects/loginapp/tsconfig.app.json",
            "assets": [
              "projects/loginapp/src/favicon.ico",
              "projects/loginapp/src/assets"
            ],
            "styles": [
              "projects/loginapp/src/styles.css"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "loginapp:build:production"
            },
            "development": {
              "browserTarget": "loginapp:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "loginapp:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "projects/loginapp/tsconfig.spec.json",
            "assets": [
              "projects/loginapp/src/favicon.ico",
              "projects/loginapp/src/assets"
            ],
            "styles": [
              "projects/loginapp/src/styles.css"
            ],
            "scripts": []
          }
        }
      }
    },
    "shared-lib": {
      "projectType": "library",
      "root": "projects/shared-lib",
      "sourceRoot": "projects/shared-lib/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/shared-lib/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/shared-lib/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/shared-lib/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "tsConfig": "projects/shared-lib/tsconfig.spec.json",
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ]
          }
        }
      }
    }
  }
}
